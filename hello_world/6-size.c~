#include <stdio.h>

/**
 * main - Entry point
 *
 * Return: Always 0 (Success)
 */

int main(void)
{
	char a = 0;
	int i = 0;
	long l = 0;
	long long ll = 0;
	float f = 0;
	printf("%ld\n", sizeof(a));
	printf("%ld\n", sizeof(i));
	printf("%ld\n", sizeof(l));
	printf("%ld\n", sizeof(ll));
	printf("%ld\n", sizeof(f));
	return (0);
}
